{
  "manifest_version": 3,
  "name": "TabO. Tab Organizer",
  "version": "1.0",
  "description": "Makes it easier to navigate your active tabs!",
  "action": {
    "default_icon": "icon.png",
    "default_title": "Tab Organizer",
    "default_popup": "popup/popup.html"
  },
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "permissions": [
    "tabs",
    "activeTab"
  ]
}
